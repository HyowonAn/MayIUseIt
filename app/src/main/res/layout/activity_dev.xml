<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="net.uprin.mayiuseit.activity.DevActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <android.support.v7.widget.AppCompatButton
        android:id="@+id/delete_access_token_btn"
        style="@style/defaultButtonTheme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="(Dev)Access Token 삭제" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/dev_back_to_splash"
        style="@style/defaultButtonTheme"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="(Dev)Splash Layout으로 돌아가기" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
